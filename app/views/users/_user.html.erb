<div class="card bg-body-secondary">
  <h4 class="card-header"><strong>Name:</strong> <%= user.name %></h4>
  <div class="card-body">
    <p class="card-text mt-0">
      <strong>Email:</strong>
      <%= user.email %></p>
    </p>
    <p class="card-text">
      <strong>Phone:</strong>
      <%= user.phone %></p>
    </p>
    <p class="card-text">
      <strong>CPF:</strong>
      <%= user.cpf %></p>
    </p>
  </div>
</div>
